process {
    withName: CTAT_LR_FUSION {
        
        memory = 100.GB
        
        time = 24.h
        
        cpus = 1

        errorStrategy = { task.exitStatus in 137..140 ? 'retry' : 'terminate' }
        maxRetries = 3

        publishDir = [path:{"${params.outdir}/CTAT/${meta.id}/"}, mode: 'copy']
    


    }
}